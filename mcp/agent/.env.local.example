# Gemini API Key (set this OR Vertex credentials)
GEMINI_API_KEY=your-api-key-here

# Gemini model name (optional)
GEMINI_MODEL=gemini-3-pro-preview

# Google Application Credentials (optional for Vertex AI)
# Can be a file path or raw JSON string.
GOOGLE_APPLICATION_CREDENTIALS=

# MCP Server URL
MCP_URL=http://localhost:8080/api/v1/stream/mcp

# MCP auth token (optional)
MCP_TOKEN=

# Optional UI auth settings
UI_USER=
UI_PASSWORD=

# Optional TLS material
SSL_CERT_PEM=
SSL_KEY_PEM=
